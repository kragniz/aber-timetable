#!/usr/bin/env python
import re

if __name__ == '__main__':
    import sys
    args = sys.argv
    if len(args) > 0:
        with open(args[1]) as f:
            days = re.findall(
                r'<tr>.+?<td class="bold_label".+?</tr>',
                f.read(), re.DOTALL)
            for i, day in enumerate(days):
                for i, time in enumerate(day.split('\n')[2:11]): print i, time
